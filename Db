/****** Object:  Database [clinick]    Script Date: 5/17/2022 3:26:38 AM ******/
CREATE DATABASE [clinick]
 CONTAINMENT = NONE
 ON  PRIMARY 
( NAME = N'clinick', FILENAME = N'C:\Program Files\Microsoft SQL Server\MSSQL12.SQLEXPRESS\MSSQL\DATA\clinick.mdf' , SIZE = 3264KB , MAXSIZE = UNLIMITED, FILEGROWTH = 1024KB )
 LOG ON 
( NAME = N'clinick_log', FILENAME = N'C:\Program Files\Microsoft SQL Server\MSSQL12.SQLEXPRESS\MSSQL\DATA\clinick_log.ldf' , SIZE = 816KB , MAXSIZE = 2048GB , FILEGROWTH = 10%)
GO

ALTER DATABASE [clinick] SET COMPATIBILITY_LEVEL = 110
GO

IF (1 = FULLTEXTSERVICEPROPERTY('IsFullTextInstalled'))
begin
EXEC [clinick].[dbo].[sp_fulltext_database] @action = 'enable'
end
GO

